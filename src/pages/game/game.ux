      })
      
      // éªŒè¯å½“å‰ç©å®¶ä½ç½®çš„åœ°å—
      const currentTile = this.terrainGrid[this.playerPosition.y] && this.terrainGrid[this.playerPosition.y][this.playerPosition.x]
