  // éªŒè¯åŠ è½½åçš„åœ°å½¢æ•°æ®
  validateLoadedTerrain() {
    console.log('ğŸ” å¼€å§‹éªŒè¯åŠ è½½åçš„åœ°å½¢æ•°æ®...')
    
    try {
      // éªŒè¯åŸºæœ¬æ•°æ®ç»“æ„
      if (!this.terrainGrid || !Array.isArray(this.terrainGrid)) {
        throw new Error('åœ°å½¢ç½‘æ ¼æ•°æ®ç»“æ„æ— æ•ˆ')
      }
      
