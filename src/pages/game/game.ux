      terrainGrid: this.terrainGrid, // åŸå§‹åœ°å½¢æ•°æ®ï¼Œç”¨äºæ¸¸æˆé€»è¾‘
      terrainGridFormatted: terrainGridForSave, // æ ¼å¼åŒ–çš„å­˜æ¡£æ•°æ®ï¼Œç”¨äºæ˜¾ç¤º
      currentBiome: this.currentBiome,
